{
    "include": ["src"],

    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "src/experimental",
        "src/typestubs"
    ],

    "ignore": ["src/oldstuff"],

    "defineConstant": {
        "DEBUG": true
    },

    "stubPath": "src/stubs",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "executionEnvironments": [{
        "root": "./*"
    }]
}
